{
  "manifest_version": 2,
  "name": "Mirmindr",
  "version": "0.1",
  "description": "Mirmindr extension",

  "browser_action": {
    "default_title": "Mirmindr",
    "default_popup": "mirmindr.html",
    "default_icon": "mirmindr-icon.png"
  },
  "permissions": ["identity", "identity.email", "storage"]
}
